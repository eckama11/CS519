function readTandH(){
    local alsAddr = 0x40;
    hardware.i2c89.write(alsAddr<<1,"\xf3");
    //imp.sleep(0.07);
    local temp = hardware.i2c89.read(alsAddr<<1, "", 3);
    hardware.i2c89.write(alsAddr<<1,"\xf5");
    //imp.sleep(60);
    local hum = hardware.i2c89.read(alsAddr<<1, "", 3);
    server.log(temp + " Temp");
    server.log(hum + " Hum");
    
        
}

readTandH();
